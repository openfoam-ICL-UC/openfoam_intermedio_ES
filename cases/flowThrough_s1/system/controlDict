FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     porousFlowFoam_s1;

startFrom       latestTime;//startTime;

startTime       0;

stopAt          endTime;

endTime         1e-1;

deltaT          1e-6;

writeControl    adjustable;//runTime;

writeInterval   5e-4;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep	yes;

maxDeltaT       1e-4;

maxCo           0.9;

