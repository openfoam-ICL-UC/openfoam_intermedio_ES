transportModel  Newtonian;

// V4/V5 electrolyte kinematic viscosity, typical values (mu 0.005 Pa s, rho 1350 kg m^-3)
nu              3.7e-6; 

// Diffusivity of binary pair
D               1.569e-5; // m^2/s        

// Turbulent Schmidt number
Sct             0.85;
