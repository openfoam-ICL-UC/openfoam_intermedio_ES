FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     porousMassTransferFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         5e-2;

deltaT          1e-6;

writeControl    adjustable;

writeInterval   5e-4;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep	yes;

maxDeltaT       1e-4;

maxCo           0.75;



